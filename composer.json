{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
        "type": "package",
        "package": {
            "name": "dinbror/blazy",
            "version": "1.8.2",
            "type": "drupal-library",
            "extra": {
              "installer-name": "blazy"
            },
            "source": {
                "type": "git",
                "url": "https://github.com/dinbror/blazy",
                "reference": "1.8.2"
            }
        }
        },
        {
            "type": "package",
            "package": {
                "name": "google/charts",
                "version": "45",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "google_charts"
                },
                "dist": {
                    "url": "https://www.gstatic.com/charts/loader.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/addtoany": "^1.12",
        "drupal/admin_toolbar": "^2.3",
        "drupal/anchor_link": "^2.0",
        "drupal/asset_injector": "^2.4",
        "drupal/back_to_top": "^1.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "^1.0",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/charts": "^3.0@beta",
        "drupal/colorbox": "^1.4",
        "drupal/colorbox_inline": "^1.1",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/contact_block": "^1.4",
        "drupal/content_access": "^1.0@alpha",
        "drupal/core": "^8.6.0",
        "drupal/core-dev": "^8.8",
        "drupal/ctools": "^3.2",
        "drupal/date_popup": "^1.0",
        "drupal/draggableviews": "^1.2",
        "drupal/dynamic_entity_reference": "^2.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/embed": "^1.0",
        "drupal/empty_page": "^3.0",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/extlink": "^1.1",
        "drupal/facets": "^1.4",
        "drupal/features": "^3.8",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/flag": "^4.0@alpha",
        "drupal/focal_point": "^1.0",
        "drupal/fontyourface": "^3.2",
        "drupal/fraction": "^1.3",
        "drupal/fullcalendar_view": "^2.2",
        "drupal/geofield": "^1.1",
        "drupal/geofield_map": "^2.42",
        "drupal/geolocation": "^3.0",
        "drupal/google_analytics": "^2.4",
        "drupal/honeypot": "^1.29",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/imce": "^2.3",
        "drupal/libraries": "^3.0",
        "drupal/link_attributes": "^1.8",
        "drupal/linkit": "^5.0",
        "drupal/mailsystem": "^4.1",
        "drupal/menu_breadcrumb": "^1.9",
        "drupal/menu_item_extras": "^2.6",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/paragraphs": "^1.8",
        "drupal/path_redirect_import": "^1.0",
        "drupal/pathauto": "^1.3",
        "drupal/php": "^1.0",
        "drupal/profile": "^1.0",
        "drupal/protected_pages": "^1.1",
        "drupal/r4032login": "^1.1",
        "drupal/redirect": "^1.3",
        "drupal/rules": "^3.0@alpha",
        "drupal/salesforce": "^3.2",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.13",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/search_api_solr": "^4.1",
        "drupal/search_api_sorts": "^1.0",
        "drupal/search_api_spellcheck": "^3.0@beta",
        "drupal/seckit": "^1.1",
        "drupal/seo_checklist": "^4.1",
        "drupal/simple_instagram_feed": "^3.1",
        "drupal/simple_popup_blocks": "^1.2",
        "drupal/simple_sitemap": "^3.2",
        "drupal/simple_timeline": "^1.1",
        "drupal/simplenews": "^2.0@beta",
        "drupal/social_media_links": "^2.6",
        "drupal/superfish": "^1.4",
        "drupal/token_filter": "^1.1",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/ultimenu": "^2.0",
        "drupal/upgrade_rector": "^1.0@alpha",
        "drupal/upgrade_status": "^3.0",
        "drupal/video_embed_field": "^2.1",
        "drupal/views_attach_library": "^2.0",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/webform": "^5.1",
        "drupal/webform_mass_email": "^1.0",
        "drupal/webform_migrate": "^1.1",
        "drupal/webform_score": "^2.0@alpha",
        "drupal/webform_views": "^5.0",
        "drush/drush": "^10.3",
        "google/charts": "^45.0",
        "psr/container": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
            "git apply -v patches/core-htaccess_redirect_https_rule_stage_mha.patch"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },

        "patches": {
            "drupal/instagram_api": {
                "Add basic display":"patches/ig-basic-display-3101622-1.patch"
            },
            "drupal/draggableviews": {
                "Draggableviews":"https://www.drupal.org/files/issues/2019-12-11/not_working_with_group_by-draggableviews-2867348-36.patch"
            },
            "drupal/menu_item_extras": {
                "Menu Items Extras":"https://www.drupal.org/files/issues/2020-04-22/3061342-07-not-found-plugin_0.patch"
            },
            "drupal/cacheflush_cron": {
                "Cache flush Cron":"https://www.drupal.org/files/issues/2019-06-03/cacheflush_cron_fix_preset_name_-3052418-3.patch"
            },
            "drupal/entity_clone.module": {
                "Entity clone":"https://www.drupal.org/files/issues/2020-05-14/2784373_13.patch"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
