<?php

/**
 * @file
 * Contains svg_maps_france_departments.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function svg_maps_france_departments_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the svg_maps_france_departments module.
    case 'help.page.svg_maps_france_departments':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Svg Maps France departments') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function svg_maps_france_departments_theme() {
  return [
    'svg_maps_france_departments_detail' => [
      'render element' => 'element',
      'variables' => ['current' => NULL, 'value' => NULL],
    ],
    'svg_maps_france_departments' => [
      'render element' => 'element',
      'variables' => ['all' => [], 'current' => NULL, 'value' => NULL],
    ],
  ];
}
